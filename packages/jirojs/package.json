{
	"name": "jirojs",
	"version": "0.1.0",
	"description": "Parser for Ramen Jiro call strings (ラーメン二郎コール文字列パーサー)",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"lint": "tsc --noEmit"
	},
	"devDependencies": {
		"tsup": "^8",
		"typescript": "^5",
		"vitest": "^3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hayao/jirojs.git",
		"directory": "packages/jirojs"
	},
	"license": "MIT",
	"keywords": ["jiro", "ramen", "parser", "二郎", "ラーメン二郎", "コール"]
}
